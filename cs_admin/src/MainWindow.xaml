<Window x:Class="TwitchSongAdmin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:views="clr-namespace:TwitchSongAdmin.Views"
        Title="Twitch Song Admin" Height="720" Width="1100">
    <Grid>
        <TabControl>
            <TabItem Header="Queue">
                <views:QueueView DataContext="{Binding Queue}"/>
            </TabItem>
            <TabItem Header="Admin">
                <views:AdminView DataContext="{Binding Admin}"/>
            </TabItem>
            <TabItem Header="Library">
                <views:LibraryView DataContext="{Binding Library}"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>